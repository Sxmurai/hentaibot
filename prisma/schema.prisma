// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Guilds {
  id   String @id
  data String @default("{}")

  @@map("guilds")
}

model Users {
  id   String @id
  data String @default("{}")

  @@map("users")
}
